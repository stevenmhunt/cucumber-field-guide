{
    "env": {
      "browser": false,
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 4],
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "no-unused-vars": ["off"],
      "no-console": ["off"],
      "eqeqeq": ["error", "always"],
      "curly": ["error", "all"]
    }
  }